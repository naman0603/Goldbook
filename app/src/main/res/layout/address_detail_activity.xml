<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="loginModel"
            type="com.goldbookapp.model.LoginModel" />

    </data>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_10sdp">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white">

        <include layout="@layout/toolbar_layout"/>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingStart="@dimen/_10sdp"
                android:paddingEnd="@dimen/_10sdp"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:clickable="true">

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardBillingAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/gray_bg"
                    app:cardCornerRadius="@dimen/_5sdp"
                    app:cardElevation="0dp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_20sdp"
                    android:layout_gravity="center_vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_10sdp"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Billing Address"
                            android:textColor="@android:color/black"
                            android:fontFamily="@font/proxima_nova_bold"
                            android:textSize="@dimen/_14sdp"/>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/tvOfficeAddres"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                           android:textSize="@dimen/_12sdp"
                            android:hint="Office, Building, Apartment">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/txtOfficeAddres"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/gray_bg"
                                android:singleLine="true"
                                android:paddingStart="0dp"
                                android:paddingEnd="0dp"
                                android:ellipsize="end"
                                android:maxLength="30"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:inputType="textCapWords|textNoSuggestions"
                                 />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/tvAreaAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                           android:textSize="@dimen/_12sdp"
                            android:hint="Area, Colony, Street, Sector">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/txtAreaAddress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/gray_bg"
                                android:singleLine="true"
                                android:paddingStart="0dp"
                                android:paddingEnd="0dp"
                                android:ellipsize="end"
                                android:maxLength="30"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:inputType="textCapWords|textNoSuggestions"
                                 />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/tvLandmarkAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                           android:textSize="@dimen/_12sdp"
                            android:hint="Landmark">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/txtLandmarkAddress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/gray_bg"
                                android:singleLine="true"
                                android:paddingStart="0dp"
                                android:paddingEnd="0dp"
                                android:ellipsize="end"
                                android:maxLength="30"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:inputType="textCapWords|textNoSuggestions"
                                 />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/tvCountryAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
                            android:layout_marginTop="@dimen/_10sdp"

                            android:hint="Country/Region"
                            app:endIconMode="custom"
                            app:endIconDrawable="@drawable/ic_keyboard_arrow_down_white_24dp"
                            app:endIconTint="@color/drop_down_arrow_color">

                            <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                android:id="@+id/txtCountryAddress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/gray_bg"
                                android:paddingStart="0dp"
                                android:paddingEnd="0dp"
                                android:imeOptions="actionNext"
                                android:singleLine="true"
                                android:ellipsize="end"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:inputType="textCapSentences|textNoSuggestions"
                                 />

                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/tvStateAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
                            android:hint="State/Union Territory"

                            app:endIconMode="custom"
                            app:endIconDrawable="@drawable/ic_keyboard_arrow_down_white_24dp"
                            app:endIconTint="@color/drop_down_arrow_color">

                            <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                android:id="@+id/txtStateAddress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/gray_bg"
                                android:paddingStart="0dp"
                                android:paddingEnd="0dp"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:inputType="textCapSentences|textNoSuggestions"
                                android:singleLine="true"
                                android:ellipsize="end"
                                android:imeOptions="actionNext"
                                 />

                        </com.google.android.material.textfield.TextInputLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvCityAddress"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.6"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="City"
                                style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
                                app:endIconMode="custom"
                                app:endIconDrawable="@drawable/ic_keyboard_arrow_down_white_24dp"
                                app:endIconTint="@color/drop_down_arrow_color">

                                <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                    android:id="@+id/txtCityAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="textCapSentences|textNoSuggestions"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:imeOptions="actionNext"
                                     />

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvPinAddress"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.4"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="PIN Code">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtPinAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="number"
                                    android:maxLength="6"
                                    android:digits="0123456789"
                                    android:imeOptions="actionNext" />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvLandlineAddress"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.5"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="Landline Number">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtLandlineAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:digits="0123456789"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:imeOptions="actionNext"
                                    android:inputType="phone"
                                    android:maxLength="11"
                                     />

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvAdditionalAddress"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.5"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="Additional Number">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtAdditionalAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="phone"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:digits="0123456789"
                                    android:maxLength="11"
                                    android:imeOptions="actionNext"/>

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/tvAdditionFaxAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"

                            android:hint="Fax Number">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/txtFaxAddress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/gray_bg"
                                android:fontFamily="@font/proxima_nova_regular"
                                android:inputType="phone"
                                android:digits="+0123456789"
                                android:paddingStart="0dp"
                                android:paddingEnd="0dp"
                                android:maxLength="13"
                                android:imeOptions="actionDone" />

                        </com.google.android.material.textfield.TextInputLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Shipping Address"
                            android:layout_marginTop="@dimen/_40sdp"
                            android:textColor="@android:color/black"
                            android:fontFamily="@font/proxima_nova_bold"
                            android:textSize="@dimen/_14sdp"/>

                            <com.google.android.material.checkbox.MaterialCheckBox
                                android:id="@+id/checkSameAsBillingAddress"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:minWidth="0dp"
                                android:minHeight="0dp"
                                android:buttonTint="@color/colorPrimary"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:textSize="@dimen/_12sdp"
                                android:textColor="@android:color/black"
                                android:text="Same as billing address"
                                android:fontFamily="@font/proxima_nova_regular"/>


                        <LinearLayout
                            android:id="@+id/linearShippingAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvOfficeAddresShipping"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="Office, Building, Apartment">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtOfficeAddresShipping"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:singleLine="true"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:ellipsize="end"
                                    android:maxLength="30"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="textCapWords|textNoSuggestions"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvAreaAddressShipping"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="Area, Colony, Street, Sector">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtAreaAddressShipping"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:maxLength="30"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="textCapWords|textNoSuggestions"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvLandmarkAddressShipping"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="Landmark">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtLandmarkAddressShipping"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:maxLength="30"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="textCapWords|textNoSuggestions"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvCountryAddressShipping"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"

                                style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
                                android:hint="Country/Region"
                                app:endIconMode="custom"
                                app:endIconDrawable="@drawable/ic_keyboard_arrow_down_white_24dp"
                                app:endIconTint="@color/drop_down_arrow_color">

                                <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                    android:id="@+id/txtCountryAddressShipping"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="textCapSentences|textNoSuggestions"
                                    android:imeOptions="actionNext"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>
                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvStateAddressShipping"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"

                                style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
                                android:hint="State/Union Territory"
                                app:endIconMode="custom"
                                app:endIconDrawable="@drawable/ic_keyboard_arrow_down_white_24dp"
                                app:endIconTint="@color/drop_down_arrow_color">

                                <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                    android:id="@+id/txtStateAddressShipping"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:singleLine="true"
                                    android:ellipsize="end"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="textCapSentences|textNoSuggestions"
                                    android:imeOptions="actionNext"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="1">

                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/tvCityAddressShipping"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.6"
                                    android:layout_marginEnd="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_10sdp"

                                    style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
                                    android:hint="City"
                                    app:endIconMode="custom"
                                    app:endIconDrawable="@drawable/ic_keyboard_arrow_down_white_24dp"
                                    app:endIconTint="@color/drop_down_arrow_color">

                                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                                        android:id="@+id/txtCityAddressShipping"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@color/gray_bg"
                                        android:paddingStart="0dp"
                                        android:paddingEnd="0dp"
                                        android:singleLine="true"
                                        android:ellipsize="end"
                                        android:fontFamily="@font/proxima_nova_regular"
                                        android:inputType="textCapSentences|textNoSuggestions"
                                        android:imeOptions="actionNext"
                                        />

                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/tvPinAddressShipping"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.4"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_10sdp"

                                    android:hint="PIN Code">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/txtPinAddressShipping"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@color/gray_bg"
                                        android:paddingStart="0dp"
                                        android:paddingEnd="0dp"
                                        android:maxLength="6"
                                        android:fontFamily="@font/proxima_nova_regular"
                                        android:inputType="number"
                                        android:digits="0123456789"
                                        android:imeOptions="actionNext"
                                        />

                                </com.google.android.material.textfield.TextInputLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="1">

                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/tvLandlineAddressShipping"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.55"
                                    android:layout_marginEnd="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_10sdp"

                                    android:hint="Landline Number">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/txtLandlineAddressShipping"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@color/gray_bg"
                                        android:paddingStart="0dp"
                                        android:paddingEnd="0dp"
                                        android:fontFamily="@font/proxima_nova_regular"
                                        android:inputType="phone"
                                        android:digits="0123456789"
                                        android:maxLength="11"
                                        android:imeOptions="actionNext"
                                        />

                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/tvAdditionalAddressShipping"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.45"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:layout_marginTop="@dimen/_10sdp"

                                    android:hint="Additional Number">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/txtAdditionalAddressShipping"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@color/gray_bg"
                                        android:paddingStart="0dp"
                                        android:paddingEnd="0dp"
                                        android:fontFamily="@font/proxima_nova_regular"
                                        android:inputType="phone"
                                        android:digits="0123456789"
                                        android:maxLength="11"
                                        android:imeOptions="actionNext"
                                        />

                                </com.google.android.material.textfield.TextInputLayout>

                            </LinearLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/tvAdditionFaxAddressShipping"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"

                                android:hint="Fax Number">

                                <com.google.android.material.textfield.TextInputEditText
                                    android:id="@+id/txtFaxAddressShipping"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@color/gray_bg"
                                    android:paddingStart="0dp"
                                    android:paddingEnd="0dp"
                                    android:fontFamily="@font/proxima_nova_regular"
                                    android:inputType="phone"
                                    android:digits="+0123456789"
                                    android:maxLength="13"
                                    android:imeOptions="actionDone"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                    </LinearLayout>



                </com.google.android.material.card.MaterialCardView>



            </LinearLayout>

        </ScrollView>


    </LinearLayout>
        <com.robertlevonyan.views.customfloatingactionbutton.FloatingActionButton
            android:id="@+id/btnSaveAdd_AddressDetail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_margin="@dimen/_15sdp"
            android:fontFamily="@font/proxima_nova_bold"
            android:padding="@dimen/_5sdp"
            android:text="Save"
            android:textColor="@android:color/white"
            android:textSize="@dimen/_13sdp"
            app:fabRippleColor="@color/gray_text"
            app:fabElevation="5dp"
            app:fabIconPosition="start"
            app:fabSizes="normal"
            app:fabType="circle" />

    </RelativeLayout>
</layout>